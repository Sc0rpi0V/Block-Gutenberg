{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "Gutenberg/highlight-pdf",
	"version": "0.1.0",
	"title": "Mise en avant PDF",
	"category": "Gutenberg-block",
	"icon": "pdf",
	"description": "Mise en avant d'un fichier PDF avec contenu.",
	"attributes": {
		"pictureId": {
			"type": "number",
			"default": null
		},
		"pictureSrc": {
			"type": "string",
			"default": ""
		},
		"pictureAlt": {
			"type": "string",
			"default": ""
		},
		"category": {
			"type": "string",
			"default": ""
		}, 
		"tag" : {
			"type": "string",
			"default": ""
		},
		"overtitle": {
			"type": "string",
			"default": ""
		},
		"overtitleLevel": {
			"type": "string",
			"default": "2"
		},
		"title": {
			"type": "string",
			"default": ""
		}, 
		"titleLevel": {
			"type": "string",
			"default": "3"
		}, 
		"pdfUrl": {
			"type": "string",
			"default": ""
		},
		"pdfId": {
			"type": "number",
			"default": ""
		},
		"pdfName": {
			"type": "string",
			"default": ""
		},
		"pdfSize": {
			"type": "string",
			"default": ""
		},
		"pdfText": {
			"type": "string",
			"default": ""
		},
		"btnUrl" : {
			"type": "string",
			"default": ""
		},
		"btnText": {
			"type": "string",
			"default": ""
		},
		"btnTarget" : {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"customClassName": false
	},
	"textdomain": "Gutenberg-back",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}